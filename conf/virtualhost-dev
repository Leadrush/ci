<VirtualHost *:80>
    DocumentRoot PATH
    ServerName leadrush-debug


    <Directory PATH>

        RewriteEngine on
        RewriteCond $1 !^(index\.php|resources|robots\.txt)
        RewriteCond %{REQUEST_FILENAME} !-f
        RewriteCond %{REQUEST_FILENAME} !-d
        RewriteRule ^(.*)$ index.php/$1 [L,QSA]

        AllowOverride All
        Options FollowSymLinks
    </Directory>
</VirtualHost>